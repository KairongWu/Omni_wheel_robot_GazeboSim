<launch>
  <arg name="model" default="omni" doc="[omni]"/>
  <!-- <param name="robot_description" textfile="$(find omni_robot_description)/robots/$(arg model)_robot.urdf.xacro"/> -->
  <param name="robot_description" command="$(find xacro)/xacro --inorder '$(find omni_robot_description)/robots/$(arg model)_robot.urdf.xacro'"/>

  <node
    name="joint_state_publisher_gui"
    pkg="joint_state_publisher_gui"
    type="joint_state_publisher_gui"/>
  
  <node 
    name="robot_state_publisher"
    pkg="robot_state_publisher"
    type="robot_state_publisher"/>

  <node
    name="rviz"
    pkg="rviz"
    type="rviz"
    args="-d $(find omni_robot_description)/rviz/urdf.rviz"/>

</launch>